{
  "name": "kysely-do-root",
  "private": true,
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false
  },
  "workspaces": [
    "packages/*"
  ]
}
